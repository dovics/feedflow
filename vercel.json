{
  "buildCommand": "prisma generate && next build",
  "devCommand": "next dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["hkg1", "sin1"],
  "env": {
    "DATABASE_URL": "@database-url",
    "NEXTAUTH_SECRET": "@nextauth-secret",
    "DEFAULT_ADMIN_PASSWORD": "@default-admin-password"
  },
  "build": {
    "env": {
      "DATABASE_URL": "@database-url"
    }
  }
}
